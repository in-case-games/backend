<Window x:Class="CaseApplication.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CaseApplication.WPF"
        DataContext="{Binding MainViewModel, Source={StaticResource ViewModelLocator}}"
        mc:Ignorable="d"
        MaxHeight="{Binding WindowMeasurements.MaxHeight}"
        MaxWidth="{Binding WindowMeasurements.MaxWidth}"
        MinHeight="{Binding WindowMeasurements.MinHeight}"
        MinWidth="{Binding WindowMeasurements.MinWidth}"
        d:DesignHeight="450" d:DesignWidth="450"
        Title="MainWindow">
    <Grid>
        <Frame Content="{Binding PageSource}" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
